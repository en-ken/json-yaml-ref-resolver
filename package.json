  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^11.13.7",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint-config-prettier": "^1.18.0",
